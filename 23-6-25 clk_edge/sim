    %end;
    .thread T_0;
    .scope S_0x556203564970;
T_1 ;
    %wait E_0x55620352b590;
    %wait E_0x55620352b7e0;
    %load/vec4 v0x556203576300_0;
    %inv;
    %assign/vec4 v0x556203576300_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5562035647e0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556203576440_0, 0, 1;
T_2.0 ;
    %delay 5, 0;
    %load/vec4 v0x556203576440_0;
    %inv;
    %store/vec4 v0x556203576440_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x5562035647e0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556203576510_0, 0, 1;
T_3.0 ;
    %delay 10, 0;
    %load/vec4 v0x556203576510_0;
    %inv;
    %store/vec4 v0x556203576510_0, 0, 1;
    %jmp T_3.0;
    %end;
