v0x556203576300_0 .var "clk_2x_reg", 0 0;
E_0x55620352b590 .event posedge, v0x55620352acf0_0;
E_0x55620352b7e0 .event posedge, v0x5562035761a0_0;
    .scope S_0x556203564970;
T_0 ;
    %pushi/vec4 0, 0, 1;
